{
  "function_score": {
    "query": {
      "bool": {
        "should": [
          {
            "query_string": {
              "query": "$INPUT",
              "analyzer": "whitespace_lowercase",
              "fields": [
                "evaluationDatasets^0.065",
                "trainingDatasets^0.075",
                "description^0.035",
                "platform^0.055",
                "type^0.01"
              ],
              "default_operator": "OR"
            }
          },
          {
            "query_string": {
              "query": "$INPUT",
              "analyzer": "whitespace_lowercase",
              "boost": 0.125,
              "fields": [
                "name.ngram^0.125",
                "name.ml_model_pattern_ngram^8",
                "name.delimited^1",
                "name.pattern^8",

              ],
              "default_operator": "AND"
            }
          },
          {
            "query_string": {
              "query": "$INPUT",
              "analyzer": "custom_keyword",
              "boost": 1000000,
              "default_field": "name",
              "default_operator": "AND"
            }
          }
        ]
      }
    },
    "functions": [
      {
        "filter": {
          "term": {
            "hasOwners": true
          }
        },
        "weight": 2
      },
      {
        "filter": {
          "term": {
            "active": false
          }
        },
        "weight": 0.0001
      }
    ],
    "score_mode": "multiply"
  }
}
