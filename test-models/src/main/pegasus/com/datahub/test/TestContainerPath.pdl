namespace com.datahub.test

/**
 * Shared aspect containing Container Paths to be indexed for an entity.
 */
@Aspect = {
    "name": "testContainerPath"
}
record TestContainerPath {
  /**
   * A valid container path for the entity. This field is provided by DataHub by default.
   *
   * Container paths are stored in elasticsearch as unit-separator delimited strings and only include platform specific folders or containers.
   * These paths should not include high level info captured elsewhere ie. Platform and Environment.
   */
   @Searchable = {
    "/*/id": {
      "fieldName": "containerPath",
      "fieldType": "CONTAINER_PATH",
    }
  }
  path: array[ContainerPathEntry]
}
