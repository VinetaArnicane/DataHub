{
  "settings": {
    "index": {
      "analysis": {
        "filter": {
          "autocomplete_filter": {
            "type": "edge_ngram",
            "min_gram": "3",
            "max_gram": "50"
          },
          "custom_delimiter": {
            "split_on_numerics": "false",
            "split_on_case_change": "false",
            "type": "word_delimiter",
            "preserve_original": "true",
            "catenate_words": "false"
          }
        },
        "char_filter": {
          "ml_model_pattern": {
            "pattern": "[.]",
            "type": "pattern_replace",
            "replacement": "/"
          }
        },
        "normalizer": {
          "my_normalizer": {
            "filter": [
              "lowercase"
            ],
            "type": "custom"
          }
        },
        "analyzer": {
          "whitespace_lowercase": {
            "filter": [
              "lowercase"
            ],
            "tokenizer": "whitespace"
          },
          "slash_pattern": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "slash_tokenizer"
          },
          "ml_model_pattern": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "ml_model_pattern"
          },
          "custom_browse": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "path_hierarchy_tokenizer"
          },
          "custom_ngram": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "custom_ngram"
          },
          "custom_keyword": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "keyword"
          },
          "delimit": {
            "filter": [
              "lowercase",
              "custom_delimiter"
            ],
            "tokenizer": "whitespace"
          },
          "ml_model_pattern_ngram": {
            "filter": [
              "lowercase",
              "autocomplete_filter"
            ],
            "type": "custom",
            "tokenizer": "ml_model_pattern"
          },
          "custom_browse_slash": {
            "filter": [
              "lowercase"
            ],
            "type": "custom",
            "tokenizer": "path_hierarchy"
          }
        },
        "tokenizer": {
          "path_hierarchy_tokenizer": {
            "type": "path_hierarchy",
            "replacement": "/",
            "delimiter": "."
          },
          "custom_ngram": {
            "type": "ngram",
            "min_gram": "3",
            "max_gram": "50"
          },
          "slash_tokenizer": {
            "pattern": "[/]",
            "type": "pattern"
          },
          "ml_model_pattern": {
            "pattern": "[./]",
            "type": "pattern"
          }
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "browsePaths": {
        "type": "text",
        "fields": {
          "length": {
            "type": "token_count",
            "analyzer": "slash_pattern"
          }
        },
        "analyzer": "custom_browse_slash",
        "fielddata": true
      },
      "origin": {
        "type": "keyword",
        "fields": {
          "ngram": {
            "type": "text",
            "analyzer": "custom_ngram"
          }
        },
        "normalizer": "my_normalizer"
      },
      "active": {
        "type": "boolean"
      },
      "hasOwners": {
        "type": "boolean"
      },
      "name": {
        "type": "keyword",
        "fields": {
          "model_pattern_ngram": {
            "type": "text",
            "analyzer": "model_pattern_ngram"
          },
          "delimited": {
            "type": "text",
            "analyzer": "delimit"
          },
          "ngram": {
            "type": "text",
            "analyzer": "custom_ngram"
          },
          "pattern": {
            "type": "text",
            "analyzer": "model_pattern"
          }
        },
        "normalizer": "my_normalizer"
      },
      "owners": {
        "type": "keyword",
        "normalizer": "my_normalizer"
      },
      "platform": {
        "type": "keyword",
        "fields": {
          "ngram": {
            "type": "text",
            "analyzer": "custom_ngram"
          }
        },
        "normalizer": "my_normalizer"
      },
      "removed": {
        "type": "boolean"
      },
      "urn": {
        "type": "keyword",
        "normalizer": "my_normalizer"
      },
      "evaluationDatasets": {
        "type": "keyword",
        "normalizer": "my_normalizer"
      },
      "trainingDatasets": {
        "type": "keyword",
        "normalizer": "my_normalizer"
      },
      "type": {
        "type": "keyword",
        "fields": {
          "model_pattern_ngram": {
            "type": "text",
            "analyzer": "model_pattern_ngram"
          },
          "delimited": {
            "type": "text",
            "analyzer": "delimit"
          },
          "ngram": {
            "type": "text",
            "analyzer": "custom_ngram"
          },
          "pattern": {
            "type": "text",
            "analyzer": "model_pattern"
          }
        }
      }
    }
  }
}
