namespace com.linkedin.timeseries

/**
 * Provides the aggregation specification on a member/field.
 */
record AggregationSpec {
  /**
   * Name of the member/field.
   */
  memberName: string

  /**
   * Aggregation type for the metric.
   */
  aggregationType: AggregationType

  /**
   * Param k for top_hits
   */
  k: optional int

  /**
   * Param by member for top_hits
   */
  byMember: optional string
}