namespace com.linkedin.domain

/**
 * Information about a Domain
 */
@Aspect = {
  "name": "domainInfo"
}
record DomainInfo {

  /**
   * Display name of the Domain
   */
  @Searchable = {
    "fieldType": "TEXT_PARTIAL",
    "enableAutocomplete": true,
    "boostScore": 10.0
  }
  name: string

  /**
   * Description of the Domain
   */
  description: optional string

}
