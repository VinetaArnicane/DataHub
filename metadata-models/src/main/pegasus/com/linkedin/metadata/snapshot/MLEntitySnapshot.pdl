namespace com.linkedin.metadata.snapshot

import com.linkedin.common.MLEntityUrn
import com.linkedin.metadata.aspect.MLEntityAspect

record MLEntitySnapshot {

  /**
   * URN for the entity the metadata snapshot is associated with.
   */
  urn: MLEntityUrn

  /**
   * The list of metadata aspects associated with the MLEntity. Depending on the use case, this can either be all, or a selection, of supported aspects.
   */
  aspects: array[MLEntityAspect]
}
