namespace com.linkedin.metadata.snapshot

import com.linkedin.common.JobUrn
import com.linkedin.metadata.aspect.JobAspect

/**
 * A metadata snapshot for a specific Job entity.
 */
record JobSnapshot {

  /**
   * URN for the entity the metadata snapshot is associated with.
   */
  urn: JobUrn

  /**
   * The list of metadata aspects associated with the Job. Depending on the use case, this can either be all, or a selection, of supported aspects.
   */
  aspects: array[JobAspect]
}