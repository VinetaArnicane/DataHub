namespace com.linkedin.metadata.search

/**
 * Features to be used for search ranking
 */
@Aspect = {
  "name": "searchFeatures"
}
record SearchFeatures {
  /**
   * Whether the entity has been removed (soft-deleted).
   */
  @SearchScore = {
    "modifier": "LOG"
  }
  usageCount: optional long

  @SearchScore = {}
  hasOwner: optional int
}