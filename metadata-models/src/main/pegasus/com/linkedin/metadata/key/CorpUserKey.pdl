namespace com.linkedin.metadata.key

/**
 * Key for a CorpUser
 */
@Aspect = {
  "name": "corpUserKey",
  "isKey": true
}
record CorpUserKey {
  /**
  * The name of the AD/LDAP user.
  */
  @Searchable = {
    "fieldName": "ldap",
    "fieldType": "TEXT_WITH_PARTIAL_MATCHING",
    "queryByDefault": true,
    "boostScore": 2.0
  }
  username: string
}