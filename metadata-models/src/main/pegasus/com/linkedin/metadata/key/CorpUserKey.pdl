namespace com.linkedin.metadata.key

/**
 * Key for a CorpUser
 */
@Aspect = {
  "name": "corpUserKey",
  "isKey": true
}
record CorpUserKey {
  /**
  * The name of the AD/LDAP user.
  */
  @Searchable = {
    "fieldName": "ldap",
    "indexSettings": [{ "indexType": "TEXT" }, { "indexType": "PARTIAL" }]
  }
  username: string
}