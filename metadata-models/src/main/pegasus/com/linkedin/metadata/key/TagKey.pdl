namespace com.linkedin.metadata.key

/**
 * Key for a Tag
 */
@Aspect = {
    "name": "tagKey",
    "isKey": true
}
record TagKey {
    /**
    * The unique tag name
    */
    @Searchable = {
        "fieldName": "name",
        "indexSettings": [{ "indexType": "KEYWORD_LOWERCASE" }, { "indexType": "TEXT" }, { "indexType": "PARTIAL_SHORT" }]
    }
    name: string
}