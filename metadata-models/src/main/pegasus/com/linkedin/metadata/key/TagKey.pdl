namespace com.linkedin.metadata.key

/**
 * Key for a Tag
 */
@Aspect = {
  "name": "tagKey",
  "isKey": true
}
record TagKey {
  /**
  * The unique tag name
  */
  @Searchable = {
    "fieldType": "TEXT_WITH_PARTIAL_MATCHING",
    "queryByDefault": true,
    "enableAutocomplete": true,
    "boostScore": 10.0
  }
  name: string
}