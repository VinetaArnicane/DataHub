namespace com.linkedin.schema

import com.linkedin.common.GlobalTags

/**
 * SchemaField to describe metadata related to dataset schema. Schema normalization rules: http://go/tms-schema
 */
record EditableSchemaFieldInfo {
  /**
   * FieldPath uniquely identifying the SchemaField this metadata is associated with
   */
  fieldPath: optional string

  /**
   * Description
   */
  description: optional string

  /**
   * Tags associated with the field
   */
  globalTags: optional GlobalTags
}
